#!/bin/bash
urouter_dir="/urouter"
source $urouter_dir/urouter.uconf

if [ "$tunnelbrokerenableduconf" = 1 ];
then
date "+DATE: %m/%d/%y%nTIME: %H:%M:%S" >> $urouter_dir/logs/tunnelbroker.txt
ip route del $tunnelbrokertunnelipuconf > /dev/null
ip route add $tunnelbrokertunnelipuconf dev $internetinterfaceconf > /dev/null
ip route del $ipv4tunnelbrokernetuconf > /dev/null
ip route add $ipv4tunnelbrokernetuconf dev $internetinterfaceconf > /dev/null
curl -s $tunnelbrokerupdateurluconf --interface $internetinterfaceconf --insecure >> $urouter_dir/logs/tunnelbroker.txt

source $ulang
echo $O6vIKu && echo $O6vIKu | $uconfspeak
fi